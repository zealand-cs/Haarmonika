<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.collections.FXCollections?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="dk.haarmonika.haarmonika.controllers.BookingController">
            prefHeight="400.0" prefWidth="600.0">

    <ComboBox fx:id="dropdownMenu" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10"
              onAction="#handleComboBoxSelection">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Booking Page"/>
                <String fx:value="Customer Page"/>
                <String fx:value="Service Page"/>
                <String fx:value="Employee Page"/>
            </FXCollections>
        </items>
    </ComboBox>
</AnchorPane>
